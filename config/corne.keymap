#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
// #include <dt-bindings/zmk/mouse.h>
#include <dt-bindings/zmk/outputs.h>
#include <dt-bindings/zmk/ext_power.h>
#include <dt-bindings/zmk/rgb.h>
#include <dt-bindings/led/led.h>

#define DFL  0
#define GML  1
#define GAL  2
#define DEL  3
#define EPL  4
#define ZEL  5
#define SDL  6
#define SUL  7
#define LWL  8
#define REL  9
#define RSL 10
#define AJL 11
#define SSL 12
#define HSL 13
#define KSL 14
#define CSL 15
#define SLL 16
#define FSL 17
#define BHL 18
#define WHL 19
#define WPL 20

&spi3 {
  compatible = "nordic,nrf-spim";

  status = "okay";

  pinctrl-0 = <&spi3_default>;
  pinctrl-1 = <&spi3_sleep>;
  pinctrl-names = "default", "sleep";

  led_strip: ws2812@0 {
    compatible = "worldsemi,ws2812-spi";
    label = "SK6812mini";

    /* SPI */
    reg = <0>; /* ignored, but necessary for SPI bindings */
    spi-max-frequency = <4000000>;

    /* WS2812 */
    chain-length = <27>; /* There are per-key RGB, but the first 6 are underglow */
    spi-one-frame = <0x70>;
    spi-zero-frame = <0x40>;

    color-mapping = <LED_COLOR_ID_GREEN LED_COLOR_ID_RED LED_COLOR_ID_BLUE>;
  };
};

/ {

  chosen {
    zmk,underglow = &led_strip;
  };

  behaviors {
    lt: behavior_layer_tap { quick-tap-ms = <180>; };
    mt: behavior_mod_tap { tapping-term-ms = <180>; };
  };

  conditional_layers {
    compatible = "zmk,conditional-layers";
    realspace_layer {
      if-layers = <SUL SDL>;
      then-layer = <RSL>;
    };
    adjust_layer {
      if-layers = <LWL REL>;
      then-layer = <AJL>;
    };
    subspace_layer {
      if-layers = <SDL LWL>;
      then-layer = <SSL>;
    };
    hyperspace_layer {
      if-layers = <SUL REL>;
      then-layer = <HSL>;
    };
    knownspace_layer {
      if-layers = <SDL REL>;
      then-layer = <KSL>;
    };
    cyberspace_layer {
      if-layers = <LWL SUL>;
      then-layer = <CSL>;
    };
    slipspace_layer {
      if-layers = <SDL AJL>;
      then-layer = <SLL>;
    };
    foldspace_layer {
      if-layers = <AJL SUL>;
      then-layer = <FSL>;
    };
    blackhole_layer {
      if-layers = <SDL AJL>;
      then-layer = <BHL>;
    };
    wormhole_layer {
      if-layers = <SUL AJL>;
      then-layer = <WHL>;
    };
    warp_layer {
      if-layers = <RSL AJL>;
      then-layer = <WPL>;
    };
  };

