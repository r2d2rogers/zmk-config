#define _DF  0 //  0 DEFAULT
#define _GM  1 //  1 GAMING
#define _GA  2 //  2 GAMMA
#define _DE  3 //  3 DELTA
#define _EP  4 //  4 EPSILON
#define _ZE  5 //  5 ZETA
#define _SD  6 //  6 SPACEDOWN
#define _SU  7 //  7 SPACEUP
#define _LW  8 //  8 LOWER
#define _RE  9 //  9 RAISE
#define _RS 10 // 10 REALSPACE
#define _AJ 11 // 11 ADJUST
#define _SS 12 // 12 SUBSPACE
#define _HS 13 // 13 HYPERSPACE
#define _KS 14 // 14 KNOWNSPACE
#define _CS 15 // 15 CYBERSPACE
#define _SL 16 // 16 SLIPSPACE
#define _FS 17 // 17 FOLDSPACE
#define _BH 18 // 18 BLACKHOLE
#define _WH 19 // 19 WORMHOLE
#define _WP 20 // 20 WARP

&spi1 {
  compatible = "nordic,nrf-spim";
  /* Cannot be used together with i2c0. */
  status = "okay";
  mosi-pin = <6>;
  // Unused pins, needed for SPI definition, but not used by the ws2812 driver itself.
  sck-pin = <5>;
  miso-pin = <7>;

  led_strip: ws2812@0 {
    compatible = "worldsemi,ws2812-spi";
    label = "SK6812mini";

    /* SPI */
    reg = <0>; /* ignored, but necessary for SPI bindings */
    spi-max-frequency = <4000000>;

    /* WS2812 */
    chain-length = <27>; /* There are per-key RGB, but the first 6 are underglow */
    spi-one-frame = <0x70>;
    spi-zero-frame = <0x40>;

    color-mapping = <LED_COLOR_ID_GREEN LED_COLOR_ID_RED LED_COLOR_ID_BLUE>;
  };
};

/ {

  chosen {
    zmk,underglow = &led_strip;
  };

  behaviors {
    lt: behavior_layer_tap { quick-tap-ms = <180>; };
    mt: behavior_mod_tap { tapping-term-ms = <180>; };
  };

  conditional_layers {
    compatible = "zmk,conditional-layers";
    realspace_layer {
      if-layers = <_SU _SD>;
      then-layer = <_RS>;
    };
    adjust_layer {
      if-layers = <_LW _RE>;
      then-layer = <_AJ>;
    };
    subspace_layer {
      if-layers = <_SD _LW>;
      then-layer = <_SS>;
    };
    hyperspace_layer {
      if-layers = <_SU _RE>;
      then-layer = <_HS>;
    };
    knownspace_layer {
      if-layers = <_SD _RE>;
      then-layer = <_KS>;
    };
    cyberspace_layer {
      if-layers = <_LW _SU>;
      then-layer = <_CS>;
    };
    slipspace_layer {
      if-layers = <_SD _AJ>;
      then-layer = <_SL>;
    };
    foldspace_layer {
      if-layers = <_AJ _SU;
      then-layer = <_FS>;
    };
    blackhole_layer {
      if-layers = <_SD _AJ>;
      then-layer = <_BH>;
    };
    wormhole_layer {
      if-layers = <_SU _AJ>;
      then-layer = <_WH>;
    };
    warp_layer {
      if-layers = <_RS _AJ>;
      then-layer = <_WP>;
    };
  };

